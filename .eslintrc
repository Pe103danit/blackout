{
    "parser": "@babel/eslint-parser",
    "env": {
      "browser": true,
      "mocha": true
    },
    "extends": [
      "standard",
      "react-app"
    ],
    "rules": {
      "no-trailing-spaces": ["error", { "skipBlankLines": true } ],
      "no-return-assign": 0,
      "eol-last": 0,
      "semi": 0,
      "indent": "off",
      "camelcase": 0,
      "object-curly-spacing": 0,
      "comma-dangle": 0
    },
    "settings": {
      "react": {
        "createClass": "createReactClass",
        "pragma": "React",
        "version": "18.2.0",
        "flowVersion": "0.86"
      },
      "propWrapperFunctions": [ "forbidExtraProps" ]
    }
  }
